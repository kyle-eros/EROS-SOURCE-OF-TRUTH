\f5\b0\fs26 \cf0 \
-- layer_03_foundation.v_page_tiering_7d\
WITH base AS (\
  SELECT\
    page_id,\
    SAFE.LOG10(1 + active_fans_7d) AS lfans,\
    message_net_7d,\
    avg_earn_per_fan_7d,\
    avg_spend_per_txn_7d,\
    total_earnings_7d,\
    renew_on_rate_7d,\
    expired_change_7d\
  FROM core.page_health_7d  -- your weekly/rolling view\
),\
z AS (\
  SELECT\
    page_id,\
    (lfans - AVG(lfans) OVER()) / NULLIF(STDDEV_SAMP(lfans) OVER(),0) AS z_active,\
    (message_net_7d - AVG(message_net_7d) OVER()) / NULLIF(STDDEV_SAMP(message_net_7d) OVER(),0) AS z_msgnet,\
    (avg_earn_per_fan_7d - AVG(avg_earn_per_fan_7d) OVER()) / NULLIF(STDDEV_SAMP(avg_earn_per_fan_7d) OVER(),0) AS z_apf,\
    (avg_spend_per_txn_7d - AVG(avg_spend_per_txn_7d) OVER()) / NULLIF(STDDEV_SAMP(avg_spend_per_txn_7d) OVER(),0) AS z_spend,\
    (total_earnings_7d - AVG(total_earnings_7d) OVER()) / NULLIF(STDDEV_SAMP(total_earnings_7d) OVER(),0) AS z_total,\
    (renew_on_rate_7d - AVG(renew_on_rate_7d) OVER()) / NULLIF(STDDEV_SAMP(renew_on_rate_7d) OVER(),0) AS z_renew,\
    ((-expired_change_7d) - AVG(-expired_change_7d) OVER()) / NULLIF(STDDEV_SAMP(-expired_change_7d) OVER(),0) AS z_churn\
  FROM base\
),\
score AS (\
  SELECT\
    page_id,\
    0.30*z_active + 0.20*z_msgnet + 0.15*z_apf + 0.10*z_spend + 0.15*z_total + 0.05*z_renew + 0.05*z_churn AS intensity\
  FROM z\
),\
ptiles AS (\
  SELECT\
    APPROX_QUANTILES(intensity, 100)[OFFSET(40)] AS q40,\
    APPROX_QUANTILES(intensity, 100)[OFFSET(60)] AS q60,\
    APPROX_QUANTILES(intensity, 100)[OFFSET(80)] AS q80,\
    APPROX_QUANTILES(intensity, 100)[OFFSET(90)] AS q90\
  FROM score\
),\
gates AS (\
  SELECT\
    APPROX_QUANTILES(active_fans_7d, 100)[OFFSET(70)] AS af_p70,\
    APPROX_QUANTILES(message_net_7d, 100)[OFFSET(60)] AS msg_p60,\
    APPROX_QUANTILES(message_net_7d, 100)[OFFSET(70)] AS msg_p70,\
    APPROX_QUANTILES(total_earnings_7d, 100)[OFFSET(70)] AS tot_p70\
  FROM core.page_health_7d\
)\
SELECT\
  b.page_id,\
  s.intensity,\
  CASE\
    WHEN (s.intensity >= p.q90 AND b.active_fans_7d >= g.af_p70 AND (b.message_net_7d >= g.msg_p70 OR b.total_earnings_7d >= g.tot_p70)) THEN 'POWER'\
    WHEN (s.intensity >= p.q60 AND (b.message_net_7d >= g.msg_p60 OR b.active_fans_7d >= 0.8*g.af_p70)) THEN 'HIGH'\
    WHEN (s.intensity >= p.q40) THEN 'MED'\
    ELSE 'LOW'\
  END AS recommended_tier\
FROM base b\
JOIN score s USING (page_id)\
CROSS JOIN ptiles p\
CROSS JOIN gates g;\
